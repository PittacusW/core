{
  "name": "pittacusw/core",
  "description": "Core modifications for laravel",
  "keywords": [
    "laravel",
    "artisan"
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^7.0|^8.0|^9.0",
    "spatie/laravel-github-webhooks": "^1.2",
    "watson/rememberable": "^6.0",
    "webpatser/laravel-uuid": "^4.0",
    "laravel/framework": "^9.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-0": {
      "Pittacusw\\Core": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pittacusw\\Core\\Providers\\CoreServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
